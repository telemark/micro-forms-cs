  "name": "micro-forms-cs",
  "alias": [
    "cs.next.t-fk.no",
    "forms.cs.micro.t-fk.no"
  ],
  "type": "docker",
  "env": {
    "NODE_ENV": "production",
    "SSO_URL": "@micro-cs-sso-url",
