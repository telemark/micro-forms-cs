{
  "name": "micro-forms-cs",
  "description": "Forms for CS based on micro",
  "version": "1.0.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Jonas Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "contributors": [
    {
      "name": "Geir GÃ¥sodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=7.7.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm -E && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "cs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-forms-cs.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-forms-cs/issues"
  },
  "now": {
    "alias": [
      "cs.next.t-fk.no",
      "forms.cs.micro.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "ava": "0.18.2",
    "coveralls": "2.11.16",
    "nsp": "2.6.2",
    "nyc": "10.2.0",
    "standard": "9.0.2",
    "test-listen": "1.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "hogan.js": "3.0.2",
    "jsonwebtoken": "7.4.1",
    "micro": "7.3.0",
    "node-session": "1.2.1",
    "simple-encryptor": "1.1.0",
    "urlencoded-body-parser": "2.0.1"
  }
}